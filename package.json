{
  "name": "geovisto",
  "version": "1.0.0",
  "description": "TypeScript mapping library for generic geospatial data visualization based on Leaflet",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "style": "dist/index.css",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "build:webpack": "webpack --config webpack.config.js"
  },
  "keywords": [
    "gis",
    "map",
    "leaflet",
    "spatial-data",
    "visualization"
  ],
  "homepage": "https://github.com/geovisto/",
  "author": {
    "name": "Jiri Hynek",
    "url": "https://github.com/jirka"
  },
  "contributors": [
    {
      "name": "Krystof Rykala",
      "url": "https://github.com/rykala"
    },
    {
      "name": "Jakub Kachlik",
      "url": "https://github.com/kachlikjakub"
    }
  ],
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "@babel/runtime": "^7.14.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@types/d3": "^7.0.0",
    "@types/leaflet": "^1.7.4",
    "@types/node": "^12.20.17",
    "@types/rfdc": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.7",
    "eslint": "^7.31.0",
    "file-loader": "^6.2.0",
    "leaflet": "^1.7.1",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.1",
    "postcss-copy": "^7.1.0",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.54.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "sass": "^1.36.0",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.2.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "webpack": "^5.46.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "rfdc": "^1.3.0"
  },
  "peerDependencies": {
    "leaflet": "^1.7.1"
  }
}
